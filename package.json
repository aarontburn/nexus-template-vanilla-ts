{
  "name": "nexus-template-vanilla-ts",
  "version": "1.0.0",
  "description": "A developer template to create a module using vanilla TS and HTML for Nexus.",
  "author": "aarontburn",
  "scripts": {
    "build": "npm run dev_export",
    "start": "npm run build && electron ./node_modules/@nexus/nexus-client/main.js --dev --trace-warnings",
    "export": "node node_modules/@nexus/nexus-exporter/exporter.js",
    "dev_export": "node node_modules/@nexus/nexus-exporter/exporter.js --dev",
    "update-nexus": "npm update @nexus/nexus-client"
  },
  "devDependencies": {
    "@nexus/nexus-client": "https://gitpkg.vercel.app/aarontburn/nexus-core/nexus/dist?main",
    "@types/yauzl-promise": "^4.0.1",
    "electron": "^28.0.0",
    "typescript": "^4.8.3",
    "yauzl-promise": "^4.0.0"
  }
}
